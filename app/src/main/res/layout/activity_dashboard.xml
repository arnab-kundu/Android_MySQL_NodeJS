<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bg_splash"
    android:padding="15dp"
    tools:context=".activity.DashboardActivity">

    <Button
        android:id="@+id/classicmodelsdb"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentTop="true"
        android:onClick="classicmodelsdb"
        android:text="Classic Models DB"
        android:textAllCaps="false" />


    <Button
        android:id="@+id/button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="80dp"
        android:onClick="EmployeeDB"
        android:text="Employee DB"
        android:textAllCaps="false" />

    <Button
        android:id="@+id/button2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/button"
        android:layout_marginBottom="161dp"
        android:onClick="SalaryChart"
        android:text="Salary Chart"
        android:textAllCaps="false" />

    <Button
        android:id="@+id/button3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_below="@+id/button"
        android:layout_marginEnd="29dp"
        android:onClick="Country"
        android:text="Country"
        android:textAllCaps="false" />

    <Button
        android:id="@+id/button4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_marginBottom="80dp"
        android:layout_marginEnd="27dp"
        android:onClick="Car"
        android:text="Car"
        android:textAllCaps="false" />


    <Button
        android:id="@+id/button5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/button"
        android:layout_marginBottom="32dp"
        android:onClick="AddVehicle"
        android:text="AddVehicle"
        android:textAllCaps="false" />

   <!-- <com.example.akundu.android_mysql_nodejs.view.DigitalView
        android:layout_width="50dp"
        android:layout_height="100dp"
        android:layout_centerInParent="true" />-->
</RelativeLayout>